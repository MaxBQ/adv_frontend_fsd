{
  "name": "advfront",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --env port=3000",
    "start:dev:server": "node ./json-server/index.js",
    "build:prod": "webpack build --env mode=production",
    "build:dev": "webpack build --env mode=development",
    "lint:ts": "npx eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "npx eslint \"**/*.{ts,tsx}\" --fix",
    "lint:css": "npx stylelint \"**/*.scss\"",
    "lint:css:fix": "npx stylelint \"**/*.scss\" --fix",
    "test:unit": "jest --config ./configs/jest/jest.config.ts",
    "test:ui": "npx loki test",
    "test:ui:ok": "npx loki approve",
    "test:ui:report": "npm run test:ui:json && npm run test:ui:html",
    "test:ui:json": "node scripts/generate-visual-json-report.js",
    "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "test:ui:ci": "loki --requireReference --reactUri file:./storybook-static",
    "storybook": "storybook dev -p 6006 -c ./configs/storybook",
    "storybook:build": "storybook build -c ./configs/storybook",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.20",
    "@babel/preset-env": "^7.22.20",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@storybook/addon-essentials": "^7.5.0",
    "@storybook/addon-interactions": "^7.5.0",
    "@storybook/addon-links": "^7.5.0",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.5.0",
    "@storybook/react": "^7.5.0",
    "@storybook/react-webpack5": "^7.5.0",
    "@storybook/testing-library": "^0.2.2",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^12.1.3",
    "@types/file-loader": "^5.0.1",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.5.7",
    "@types/react": "^17.0.65",
    "@types/react-dom": "^17.0.20",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack": "^5.28.2",
    "@types/webpack-bundle-analyzer": "^4.6.1",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/parser": "^6.9.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-i18next-extract": "^0.9.0",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.0",
    "eslint-plugin-i18next": "^6.0.3",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "json-server": "^0.17.4",
    "loki": "^0.32.0",
    "mini-css-extract-plugin": "^2.7.6",
    "react-refresh": "^0.14.0",
    "reg-cli": "^0.17.7",
    "sass": "^1.66.1",
